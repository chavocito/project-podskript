{{template "head.tmpl" .}}
{{template "nav.tmpl" .}}
<main>
    <section id="introduction">
    <h2>About Me</h2>
    <p>{{.Introduction}}</p>
    </section>
    <section id="projects">
    <h2>Projects</h2>
    <ul>
    {{range .Projects}}
    <li>
    <h3>{{.Title}}</h3>
    <p>{{.Description}}</p>
    <a href="{{.Link}}">View Project</a>
    </li>
    {{end}}
    </ul>
    </section>
</main>
<footer>
    <p>&copy; {{.Year}} {{.Name}}. All rights reserved.</p>
</footer>
</body>
</html>